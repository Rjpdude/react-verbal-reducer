{
  "name": "react-verbal-reduer",
  "version": "1.0.2",
  "author": "Ryan J. Pridgen",
  "description": "An extension of React's 'useReducer' hook to automatically type and map reducer actions.",
  "keywords": [
    "react",
    "reducer",
    "redux",
    "actions",
    "types",
    "typescript",
    "ease",
    "easy",
    "alternative",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rjpdude/react-verbal-reducer.git"
  },
  "bugs": {
    "url": "https://github.com/Rjpdude/react-verbal-reducer/issues"
  },
  "homepage": "https://github.com/Rjpdude/react-verbal-reducer#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react-dom": "^16.8.6",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  }
}
